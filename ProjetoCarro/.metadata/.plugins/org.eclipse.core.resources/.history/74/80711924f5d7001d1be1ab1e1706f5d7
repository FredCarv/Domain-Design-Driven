package br.com.fiap.beans;

public class Carro {
	//variaveis
	private String marca, modelo;
	private int ano;
	private double valor;
	private ParteCarro ParteCarro;
	
	public Carro() {
		super();
	}
	
	//metodo construtor sem atributos de referencia
	
	public Carro(String marca, String modelo, int ano, double valor, br.com.fiap.beans.ParteCarro parteCarro) {
		super();
		this.marca = marca;
		this.modelo = modelo;
		this.ano = ano;
		this.valor = valor;
		ParteCarro = parteCarro;
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}

	public String getModelo() {
		return modelo;
	}

	public void setModelo(String modelo) {
		this.modelo = modelo;
	}

	public int getAno() {
		return ano;
	}

	public void setAno(int ano) {
		this.ano = ano;
	}

	public double getValor() {
		return valor;
	}

	public void setValor(double valor) {
		this.valor = valor;
	}

	public ParteCarro getParteCarro() {
		return ParteCarro;
	}

	public void setParteCarro(ParteCarro parteCarro) {
		ParteCarro = parteCarro;
	}
	
	
	
}
